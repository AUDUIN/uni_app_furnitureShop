.address {
  height: 1334rpx;
  background-color: #091E42;
}
.head {
  padding-bottom: 42rpx;
  background: #091E42;
}
.head .head-title {
    color: #57D9A3;
    font-size: 64rpx;
    padding-left: 64rpx;
}
.head .head-user {
    text-align: center;
    line-height: 64rpx;
    margin-top: 24rpx;
    margin-right: 32rpx;
    border-radius: 50%;
    width: 64rpx;
    height: 64rpx;
    background-color: white;
}
.head .head-user image {
      width: 100%;
      height: 100%;
      border-radius: 50%;
}
.head .search {
    margin-top: 42rpx;
    position: relative;
}
.head .search image {
      position: absolute;
      width: 36rpx;
      height: 36rpx;
      top: 32rpx;
      left: 62rpx;
}
.head .search input {
      color: #FFFFFF;
      background-color: #172B4D;
      width: 586rpx;
      height: 96rpx;
      border-radius: 16rpx;
      margin: auto;
      padding-left: 100rpx;
}
.head .nav {
    margin-top: 32rpx;
    padding-bottom: 60rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-pack: center;
    -webkit-justify-content: center;
        -ms-flex-pack: center;
            justify-content: center;
}
.head .nav .nav-item {
      padding: 8rpx 36rpx;
      text-align: center;
      color: #A5ADBA;
}
.head .nav .nav-item.active {
        background-color: #57D9A3;
        color: #FFFFFF;
}
.delivery-box {
  background-color: #57D9A3;
  border-radius: 32rpx;
  position: relative;
}
.delivery-box .delivery {
    width: calc(100% - 78rpx);
    background-color: #FFFFFF;
    border-radius: 32rpx;
    overflow: hidden;
}
.delivery-box .delivery .shipping-address {
      width: 254rpx;
      height: 58rpx;
      background-color: #57D9A3;
      margin-left: 48rpx;
      margin-top: 48rpx;
      margin-bottom: 20rpx;
      text-align: center;
}
.delivery-box .delivery .shipping-address text {
        line-height: 58rpx;
        font-size: 48rpx;
        color: #FFFFFF;
}
.delivery-box .delivery form {
      display: inline-block;
      margin-bottom: 208rpx;
}
.delivery-box .delivery form input {
        font-size: 34rpx;
        line-height: 44rpx;
        margin-left: 48rpx;
        margin-right: 48rpx;
        border-bottom: 2rpx solid #DFE1E6;
        padding-bottom: 40rpx;
        padding-top: 40rpx;
        padding-left: 18rpx;
}
.delivery-box .delivery form .sel {
        width: 576rpx;
        margin-left: 48rpx;
        margin-right: 48rpx;
        padding-bottom: 40rpx;
        padding-top: 40rpx;
        padding-left: 18rpx;
        border: none;
        border-bottom: 2rpx solid #DFE1E6;
        font-size: 34rpx;
        color: #172B4D;
        outline: none;
        -webkit-appearance: none;
           -moz-appearance: none;
                appearance: none;
        background: url(data:image/png;base64,) no-repeat right center;
        background-size: 42rpx 24rpx;
}
.delivery-box .delivery form .sel-two {
        display: -webkit-box;
        display: -webkit-flex;
        display: -ms-flexbox;
        display: flex;
        -webkit-box-pack: justify;
        -webkit-justify-content: space-between;
            -ms-flex-pack: justify;
                justify-content: space-between;
        margin-left: 48rpx;
        margin-right: 48rpx;
}
.delivery-box .delivery form .sel-two .select-item {
          width: 240rpx;
          padding-bottom: 40rpx;
          padding-top: 40rpx;
          padding-left: 18rpx;
          border: none;
          border-bottom: 2rpx solid #DFE1E6;
          font-size: 34rpx;
          color: #172B4D;
          outline: none;
          -webkit-appearance: none;
             -moz-appearance: none;
                  appearance: none;
          background: url(data:image/png;base64,) no-repeat right center;
          background-size: 42rpx 24rpx;
}
.delivery-box .arrow {
    position: absolute;
    bottom: -64rpx;
    left: 50%;
    -webkit-transform: translate(-50%);
        -ms-transform: translate(-50%);
            transform: translate(-50%);
    width: 128rpx;
    height: 128rpx;
    border-radius: 50%;
    background-color: #2684FF;
    text-align: center;
    line-height: 128rpx;
}
.delivery-box .arrow image {
      width: 48rpx;
      height: 48rpx;
      vertical-align: middle;
}

